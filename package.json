{
  "name": "api-completejavascript-com",
  "version": "0.0.3",
  "description": "A collection of some public RESTful APIs from Complete Javascript",
  "main": "index.js",
  "scripts": {
    "vercel:prod": "vercel -e NODE_ENV=production --prod",
    "vercel:stg": "vercel -e NODE_ENV=preview",
    "start": "cross-env NODE_ENV=development node index.js",
    "start-dev": "cross-env NODE_ENV=development nodemon index.js",
    "test-v1": "cross-env NODE_ENV=development mocha ./test/v1/*.js"
  },
  "dependencies": {
    "body-parser": "^1.19.0",
    "cross-env": "^7.0.3",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "morgan": "^1.10.0",
    "node-fetch": "^3.1.1",
    "pug": "^3.0.2",
    "rss-parser": "^3.12.0",
    "serve-favicon": "^2.5.0",
    "swagger-jsdoc": "^6.1.0",
    "valid-url": "^1.0.9"
  },
  "devDependencies": {
    "chai": "^4.3.4",
    "chai-http": "^4.3.0",
    "mocha": "^8.4.0",
    "nodemon": "^2.0.7"
  },
  "engines": {
    "node": ">=12.0.0"
  },
  "repository": {
    "url": "https://github.com/completejavascript/api-completejavascript-com"
  },
  "author": "Lam Pham",
  "license": "MIT",
  "keywords": [
    "node",
    "public-api",
    "json-feed",
    "rss-json",
    "express"
  ]
}
