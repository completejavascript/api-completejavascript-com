{
  "name": "api-completejavascript-com",
  "version": "0.0.2",
  "description": "A collection of some public RESTful APIs from Complete Javascript",
  "main": "server.js",
  "scripts": {
    "deploy": "now -e NODE_ENV=production && now alias",
    "staging": "now -e NODE_ENV=staging && now alias -A ./now.staging.json",
    "start": "node server.js",
    "start-dev": "nodemon server.js",
    "test-v1": "mocha ./test/v1/*.js"
  },
  "dependencies": {
    "body-parser": "^1.18.3",
    "config": "^3.0.1",
    "express": "^4.16.4",
    "morgan": "^1.9.1",
    "pug": "^3.0.1",
    "rss-parser": "^3.6.2",
    "serve-favicon": "^2.5.0",
    "swagger-jsdoc": "^3.2.6",
    "valid-url": "^1.0.9"
  },
  "devDependencies": {
    "chai": "^4.2.0",
    "chai-http": "^4.2.1",
    "mocha": "^5.2.0",
    "nodemon": "^1.18.9"
  },
  "engines": {
    "node": "8.x"
  },
  "repository": {
    "url": "https://github.com/completejavascript/api-completejavascript-com"
  },
  "author": "Lam Pham",
  "license": "MIT",
  "keywords": [
    "node",
    "public-api",
    "json-feed",
    "rss-json",
    "express"
  ]
}
